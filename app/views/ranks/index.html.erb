<h1>Listing ranks</h1>

<table>
  <tr>
    <th>Name</th>
	<th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ranks.each do |rank| %>
  <tr>
    <td><%= rank.name %></td>
	<td><%= rank.description %></td>
    <td><%= link_to 'Show', rank %></td>
    <td><%= link_to 'Edit', edit_rank_path(rank) %></td>
    <td><%= link_to 'Destroy', rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rank', new_rank_path %>
